{
  "$schema": "./node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "migration-v18": {
      "version": "18",
      "description": "Updates NG-YUNZAI to v18 [https://github.com/hbyunzai/ng-yunzai/issues/2502]",
      "factory": "./ng-update/index#updateToV18"
    },
    "ng-post-update": {
      "description": "Performs cleanup after ng-update.",
      "factory": "./ng-update/index#postUpdate",
      "private": true
    }
  }
}
