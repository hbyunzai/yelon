{
  "name": "yelon",
  "version": "13.5.1",
  "keywords": [
    "yelon",
    "ng-yunzai",
    "antd",
    "ng-zorro-antd",
    "angular"
  ],
  "author": "devcui <devcui@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hbyunzai/yelon/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hbyunzai/yelon.git"
  },
  "scripts": {
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "prepare": "husky install",
    "start": "npm run site:gen && ng s -o --hmr",
    "lint": "bash scripts/ci/lint.sh -ts -less",
    "lint:ts": "eslint --cache . --ext .ts,.html",
    "lint:fix": "eslint --fix --cache . --ext .ts,.html",
    "lint:style": "npx stylelint 'packages/**/*.less'",
    "test": "ng test --no-progress --browsers=ChromeHeadlessCI --code-coverage --no-watch",
    "test:cli": "bash scripts/ci/build-schematics.sh -t -clone",
    "test:integration": "bash scripts/ci/build-schematics.sh -b -t -clone -integration",
    "build": "bash scripts/ci/build.sh -packages -schematics -less -scaffold",
    "build_artifacts": "bash scripts/ci/build-artifacts.sh",
    "analyze": "npm run ng-high-memory build -- --source-map",
    "analyze:view": "source-map-explorer src/dist/browser/*.js",
    "site:gen": "node scripts/site/main init && ng-yunzai-plugin-theme -t=themeCss && ng-yunzai-plugin-theme -t=colorLess",
    "site:build": "npm run site:gen && npm run ssr:prerender && npm run site:ngsw && npm run site:minify && npm run site:sitemap && npm run site:helper",
    "site:helper": "bash scripts/ci/helper.sh",
    "site:minify": "node scripts/build/minify.js",
    "site:sitemap": "node scripts/build/sitemap.js",
    "site:ngsw": "node scripts/build/ngsw.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --pkg package.json",
    "pre-publish": "node scripts/publish/publish.js && node scripts/publish/publish-scaffold.js",
    "publish:next": "bash scripts/publish/publish.sh -next",
    "publish": "bash scripts/publish/publish.sh",
    "sync-scaffold-version": "node scripts/publish/sync-scaffold-version.js",
    "ssr": "npm run ng-high-memory run site:serve-ssr",
    "ssr:serve": "node src/dist/server/main.js",
    "ssr:build": "npm run site:build && npm run ng-high-memory run site:server:production",
    "ssr:prerender": "ng run site:prerender"
  },
  "engines": {
    "yarn": ">=1.21.1 <2",
    "npm": "Please use yarn instead of NPM to install dependencies"
  },
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/elements": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/platform-server": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@angular/service-worker": "~13.3.0",
    "@antv/data-set": "^0.11.8",
    "@antv/g2": "^4.1.47",
    "@ng-util/monaco-editor": "^13.0.0",
    "@nguniversal/express-engine": "~13.0.2",
    "@stackblitz/sdk": "^1.6.0",
    "@stomp/rx-stomp": "^1.1.4",
    "@webcomponents/custom-elements": "^1.5.0",
    "ajv": "^8.10.0",
    "ajv-formats": "^2.1.1",
    "aos": "^3.0.0-beta.6",
    "codesandbox": "^2.2.3",
    "echarts": "^5.3.1",
    "express": "^4.17.2",
    "extend": "^3.0.2",
    "file-saver": "^2.0.5",
    "isutf8": "^4.0.0",
    "ng-github-button": "^13.0.0",
    "ng-zorro-antd": "^13.3.2",
    "ngx-color": "~7.3.3",
    "ngx-countdown": "^13.0.0",
    "ngx-highlight-js": "^13.0.0",
    "ngx-tinymce": "^13.0.0",
    "ngx-ueditor": "^13.0.0",
    "qrious": "^4.0.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.3",
    "@angular-eslint/builder": "~13.1.0",
    "@angular-eslint/eslint-plugin": "~13.1.0",
    "@angular-eslint/eslint-plugin-template": "~13.1.0",
    "@angular-eslint/schematics": "~13.1.0",
    "@angular-eslint/template-parser": "~13.1.0",
    "@angular/cli": "~13.3.3",
    "@angular/compiler-cli": "~13.3.0",
    "@angular/language-service": "~13.3.0",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-angular": "^16.2.3",
    "@nguniversal/builders": "^13.0.2",
    "@types/aos": "^3.0.4",
    "@types/deep-extend": "^0.4.32",
    "@types/express": "^4.17.13",
    "@types/extend": "^3.0.1",
    "@types/file-saver": "^2.0.5",
    "@types/fs-extra": "^9.0.13",
    "@types/jasmine": "~3.10.0",
    "@types/jasminewd2": "~2.0.10",
    "@types/mockjs": "^1.0.6",
    "@types/node": "^12.11.1",
    "@types/parse5": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "~5.15.0",
    "@typescript-eslint/parser": "~5.15.0",
    "chalk": "^4.1.0",
    "codecov": "^3.8.3",
    "codelyzer": "^6.0.2",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-deprecation": "~1.3.2",
    "eslint-plugin-import": "~2.25.4",
    "eslint-plugin-jsdoc": "~38.0.4",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-prettier": "~4.0.0",
    "fs-extra": "^9.0.0",
    "html-minifier-terser": "^5.1.1",
    "husky": "^7.0.4",
    "jasmine-core": "~4.0.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jsonml.js": "^0.1.0",
    "jszip": "^3.7.1",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-parallel": "^0.3.1",
    "karma-spec-reporter": "0.0.33",
    "karma-summary-reporter": "^3.1.1",
    "karma-viewport": "^1.0.8",
    "klaw-sync": "^6.0.0",
    "less-bundle-promise": "^1.0.11",
    "less-plugin-clean-css": "^1.5.1",
    "less-plugin-npm-import": "^2.1.0",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^12.3.7",
    "mark-twain": "^2.0.3",
    "mockjs": "^1.1.0",
    "mustache": "^4.2.0",
    "ng-alain-codelyzer": "^0.0.1",
    "ng-packagr": "^13.3.0",
    "ng-yunzai-plugin-theme": "^13.0.3",
    "ng-yunzai-sts": "^0.0.3",
    "parse5": "^6.0.1",
    "pdfjs-dist": "^2.14.305",
    "plyr": "^3.6.12",
    "prettier": "^2.6.0",
    "protractor": "~7.0.0",
    "raw-loader": "^4.0.2",
    "readline-sync": "^1.4.10",
    "screenfull": "^6.0.1",
    "sitemap": "^7.1.1",
    "source-map-explorer": "^2.5.2",
    "stream": "0.0.2",
    "stylelint": "^14.6.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-order": "^5.0.0",
    "swagger-typescript-api": "^9.3.1",
    "terser": "^5.12.1",
    "ts-node": "~9.1.1",
    "tsconfig-paths": "^3.14.0",
    "typescript": "~4.6.2",
    "xlsx": "^0.18.4"
  },
  "lint-staged": {
    "(schematics|scripts|packages)/**/!(tslint-rules|testing)/!(polyfills).{html,ts}": [
      "eslint --fix"
    ],
    "packages/**/*.less": [
      "npm run lint:style"
    ]
  }
}
